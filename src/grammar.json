{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "tcl",
  "rules": {
    "tcl_script": {
      "type": "REPEAT1",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "command"
          },
          {
            "type": "SYMBOL",
            "name": "comment"
          },
          {
            "type": "SYMBOL",
            "name": "_terminator"
          },
          {
            "type": "SYMBOL",
            "name": "_space"
          }
        ]
      }
    },
    "command": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "word"
          }
        },
        {
          "type": "FIELD",
          "name": "arg_list",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "_arguments_list"
              },
              {
                "type": "BLANK"
              }
            ]
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_space"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "_terminator"
        }
      ]
    },
    "_arguments_list": {
      "type": "REPEAT1",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "_space"
          },
          {
            "type": "FIELD",
            "name": "argument",
            "content": {
              "type": "SYMBOL",
              "name": "word"
            }
          }
        ]
      }
    },
    "_terminator": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PATTERN",
          "value": "\\n"
        },
        {
          "type": "STRING",
          "value": ";"
        },
        {
          "type": "STRING",
          "value": "\u0000"
        }
      ]
    },
    "word": {
      "type": "REPEAT1",
      "content": {
        "type": "SYMBOL",
        "name": "_word"
      }
    },
    "_word": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "bare_word"
        },
        {
          "type": "SYMBOL",
          "name": "variable_substitution"
        },
        {
          "type": "SYMBOL",
          "name": "command_substitution"
        },
        {
          "type": "SYMBOL",
          "name": "double_quotes"
        },
        {
          "type": "SYMBOL",
          "name": "braces"
        }
      ]
    },
    "bare_word": {
      "type": "PATTERN",
      "value": "[^;\\s\\\"\\{\\[\\$][^\\s\\[\\$]*"
    },
    "command_substitution": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "command_without_close_brackets"
          },
          "named": true,
          "value": "command"
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "command_without_close_brackets": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "ALIAS",
            "content": {
              "type": "SYMBOL",
              "name": "word_without_close_brackets"
            },
            "named": true,
            "value": "word"
          }
        },
        {
          "type": "FIELD",
          "name": "arg_list",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "_arguments_list_without_close_brackets"
              },
              {
                "type": "BLANK"
              }
            ]
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_space"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "word_without_close_brackets": {
      "type": "REPEAT1",
      "content": {
        "type": "SYMBOL",
        "name": "_word_without_close_brackets"
      }
    },
    "_word_without_close_brackets": {
      "type": "CHOICE",
      "members": [
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "bare_word_without_close_brackets"
          },
          "named": true,
          "value": "bare_word"
        },
        {
          "type": "SYMBOL",
          "name": "variable_substitution"
        },
        {
          "type": "SYMBOL",
          "name": "command_substitution"
        },
        {
          "type": "SYMBOL",
          "name": "double_quotes"
        },
        {
          "type": "SYMBOL",
          "name": "braces"
        }
      ]
    },
    "_arguments_list_without_close_brackets": {
      "type": "REPEAT1",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "_space"
          },
          {
            "type": "FIELD",
            "name": "argument",
            "content": {
              "type": "ALIAS",
              "content": {
                "type": "SYMBOL",
                "name": "word_without_close_brackets"
              },
              "named": true,
              "value": "word"
            }
          }
        ]
      }
    },
    "bare_word_without_close_brackets": {
      "type": "PATTERN",
      "value": "[^\\s\\]\\\"\\{\\[\\$][^\\s\\]\\[\\$]*"
    },
    "double_quotes": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\""
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "double_quotes_internal"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "\""
        }
      ]
    },
    "double_quotes_internal": {
      "type": "REPEAT1",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "PATTERN",
            "value": "[^\\[\\$\\\\\\\"]+"
          },
          {
            "type": "SYMBOL",
            "name": "command_substitution"
          },
          {
            "type": "SYMBOL",
            "name": "variable_substitution"
          },
          {
            "type": "SYMBOL",
            "name": "_backslash_substitution"
          }
        ]
      }
    },
    "argument_expansion": {
      "type": "PREC",
      "value": 0,
      "content": {
        "type": "PATTERN",
        "value": "\\{\\*\\}[^\\s]"
      }
    },
    "braces": {
      "type": "PREC",
      "value": 1,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "{"
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "braces_internal"
              },
              {
                "type": "BLANK"
              }
            ]
          },
          {
            "type": "STRING",
            "value": "}"
          }
        ]
      }
    },
    "braces_internal": {
      "type": "REPEAT1",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "PATTERN",
            "value": "[^\\{\\}]+"
          },
          {
            "type": "SYMBOL",
            "name": "even_braces"
          }
        ]
      }
    },
    "even_braces": {
      "type": "PREC",
      "value": 1,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "{"
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "braces_internal"
              },
              {
                "type": "BLANK"
              }
            ]
          },
          {
            "type": "STRING",
            "value": "}"
          }
        ]
      }
    },
    "variable_substitution": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "$"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "alowed_variable_name"
            },
            {
              "type": "SYMBOL",
              "name": "braces"
            },
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "alowed_variable_name"
                },
                {
                  "type": "IMMEDIATE_TOKEN",
                  "content": {
                    "type": "STRING",
                    "value": "("
                  }
                },
                {
                  "type": "SYMBOL",
                  "name": "word"
                },
                {
                  "type": "IMMEDIATE_TOKEN",
                  "content": {
                    "type": "STRING",
                    "value": ")"
                  }
                }
              ]
            }
          ]
        }
      ]
    },
    "alowed_variable_name": {
      "type": "PATTERN",
      "value": "[a-zA-Z0-9_]+"
    },
    "_backslash_substitution": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\\"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "PATTERN",
              "value": "."
            },
            {
              "type": "PATTERN",
              "value": "\\n"
            }
          ]
        }
      ]
    },
    "_single_space": {
      "type": "PATTERN",
      "value": " "
    },
    "_single_tab": {
      "type": "PATTERN",
      "value": "\\t"
    },
    "_space": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "REPEAT1",
        "content": {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_single_space"
            },
            {
              "type": "SYMBOL",
              "name": "_single_tab"
            }
          ]
        }
      }
    },
    "comment": {
      "type": "PATTERN",
      "value": "#[^\\n]*"
    }
  },
  "extras": [],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}