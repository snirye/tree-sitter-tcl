{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "tcl",
      "camelcase": "Tcl",
      "title": "Tcl",
      "scope": "source.tcl",
      "file-types": [
        "itcl",
        "tcl"
      ],
      "injection-regex": "^tcl$",
      "class-name": "TreeSitterTcl"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Tcl parser",
    "authors": [
      {
        "name": "Snir Yehuda",
        "email": "SnirYehud@gmail.com",
        "url": "https://github.com/snirye"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-tcl"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": true
  }
}
